<div class="container">
  
	<div class="page-header clearfix">
	  <div class="pull-left">
	    <h1>Listing Assigned Addresses</h1>
	  </div>
	  <div class="pull-right btn-header">
	    <a href="<%= new_assigned_address_path %>" class="btn btn-primary">
	      <span class="glyphicon glyphicon-plus"></span>
	      New Assigned Address
	    </a>
	  </div>
	</div>
	
	<div class="row">
	  <div class="column">
	    <%= render 'search' %>
	  </div>
	</div>

	<div class="row">
	  <div class="column">
	    <%= render 'assigned_addresses' %>
	  </div>
	</div>

	<%= paginate @assigned_addresses %>
</div>