	    <h1>Listing Networks</h1>
	  </div>
	  <div class="pull-right btn-header">
	    <a href="<%= new_network_path %>" class="btn btn-primary">
	      <span class="glyphicon glyphicon-plus"></span>
	      New Network
	    </a>
	  </div>
	</div>

	<div class="row">
	  <div class="column">
